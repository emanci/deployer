{
    "name": "emanci/deployer",
    "description": "Another simple deployment tool",
    "keywords": ["deployer"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [{
        "name": "emanci",
        "email": "zhengchaopu@gmail.com"
    }],
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "3.4.x-dev"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Emanci\\Deployer\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
